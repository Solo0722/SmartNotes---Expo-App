{
  "expo": {
    "name": "Smart-Note",
    "slug": "Smartly store all your notes.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/android/mipmap-hdpi/ic_launcher.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/android/mipmap-hdpi/ic_launcher.png",
      "resizeMode": "contain",
      "backgroundColor": "#181820"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/android/mipmap-hdpi/ic_launcher.png",
        "backgroundColor": "#181820"
      }
    },
    "web": {
      "favicon": "./assets/android/mipmap-hdpi/ic_launcher.png"
    }
  }
}
